@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<http://fps.io/plugins/clipping.emahp> a lv2:Plugin ;
  doap:name "clipping.ema_high_pass";
  doap:license <http://usefulinc.com/doap/licenses/gpl> ;
  lv2:binary <hard.so> ;
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:port [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 0 ;
    lv2:symbol "in" ;
    lv2:name "In"
  ] , [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 1 ;
    lv2:symbol "out" ;
    lv2:name "Out"
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 2 ;
    lv2:symbol "time" ;
    lv2:name "Normalized Frequency";
    lv2:default 0.5 ;
    lv2:minimum 0.00001 ;
    lv2:maximum 1.0 ;
  ] .
