@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<https://fps.io/plugins/clipping.asymptotic> a lv2:Plugin ;
  doap:name "clipping.asymptotic";
  doap:license <http://usefulinc.com/doap/licenses/gpl> ;
  lv2:binary <asymptotic.so> ;
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:port [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 0 ;
    lv2:symbol "pregain" ;
    lv2:name "Pre-Gain";
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 10.0 ;
  ] , [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 1 ;
    lv2:symbol "in" ;
    lv2:name "In"
  ] , [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 2 ;
    lv2:symbol "out" ;
    lv2:name "Out"
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 3 ;
    lv2:symbol "constant" ;
    lv2:name "Normalizing constant";
    lv2:default 1.0 ;
    lv2:minimum 0.001 ;
    lv2:maximum 100.0 ;
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 4 ;
    lv2:symbol "postgain" ;
    lv2:name "Post-Gain";
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 10.0 ;
  ] .
