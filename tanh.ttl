@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<http://fps.io/plugins/clipping.tanh> a lv2:Plugin ;
  doap:name "clipping.tanh";
  doap:license <http://usefulinc.com/doap/licenses/gpl> ;
  doap:description "Implements the function output = post-gain * tanh((input_gain * input) + bias)" ;
  lv2:binary <tanh.so> ;
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:port [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 0 ;
    lv2:symbol "in" ;
    lv2:name "In"
  ] , [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 1 ;
    lv2:symbol "out" ;
    lv2:name "Out"
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 2 ;
    lv2:symbol "pregain" ;
    lv2:name "Pre-Gain (dB)";
    lv2:default 0.0 ;
    lv2:minimum -96.0 ;
    lv2:maximum 96.0 ;
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 3 ;
    lv2:symbol "bias" ;
    lv2:name "Bias";
    lv2:default 0.0 ;
    lv2:minimum -1.0 ;
    lv2:maximum 1.0 ;
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 4 ;
    lv2:symbol "postgain" ;
    lv2:name "Post-Gain (dB)";
    lv2:default 1.0 ;
    lv2:minimum -96.0 ;
    lv2:maximum 96.0 ;
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 5 ;
    lv2:symbol "drywet" ;
    lv2:name "Dry / Wet";
    lv2:default 0.5 ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 ;
  ] .
